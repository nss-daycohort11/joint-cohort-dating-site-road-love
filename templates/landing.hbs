{{#each users}}
    <div class= "col-md-2 image-container">
      <div>
        <img alt="user image"src={{user_image}}>
      </div>
      <div class="name-box">
        Name: {{user_name}}
      </div>
      <div class="age-box">
        {{age}} / {{gender}}
      </div>
      <div class="geo-box">
        {{homelocation.city}}, {{homelocation.state}}
      </div>
    </div>
{{/each}}