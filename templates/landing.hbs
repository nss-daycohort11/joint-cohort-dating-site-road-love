{{#each users}}
    <div class= "col-md-2 image-container profile_box" >
      <div>
        <img alt="user image"src="{{user_image}}">
      </div>
      <div class="name-box">
        {{user_name}}
      </div>
      <div class="age-box">
        {{age}} / {{gender}}
      </div>
      <div class="geo-box">
      {{homelocation.city}}, {{homelocation.state}}
      </div>
      <div>
      <button class="like" id="like" value={{key}}>Like</button>
      </div>
    </div>
{{/each}}